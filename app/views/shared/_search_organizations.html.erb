<%= form_tag organizations_path, method: :get do %>
	<%= select_tag :tag, options_for_select(Tag.all.pluck(:name), params[:tag]), prompt: "Search by tag" %> <br>
	<br><%= text_field_tag :q, params[:q], placeholder: "Search by name" %> <br>
	<br><%= submit_tag "Submit", class: "btn btn-primary" %> <br><br>
<% end %>