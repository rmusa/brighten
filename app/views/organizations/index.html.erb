<h1>Find an organization:</h1>

<%= render 'shared/tag_search', link: organizations_path %>

<%= render 'shared/name_search', link: organizations_path, resource: "Organizations" %>

<ul>
	<% @organizations.each do |organization| %>
	<li>
		<p><%= link_to organization.name, organization_path(organization) %></p>
		<p><%= organization.description %></p>
	</li>
	<% end %>
</ul>

<br>

<%= link_to "Back to profile", user_path(current_user) %>